</head>
<body class="u-body u-stick-footer u-xl-mode" data-lang="en">
    @*Header*@
    <header class="header-page u-border-no-bottom u-border-no-left u-border-no-right u-border-no-top u-clearfix u-header u-section-row-container" id="sec-bba3" style="background-image: none">
        <div class="u-section-rows">
            <div class="header-page u-clearfix u-section-row u-white u-section-row-1" data-animation-name="" data-animation-duration="0" data-animation-delay="0" data-animation-direction="" id="sec-655d">
                <div class="u-clearfix u-sheet u-valign-middle-lg u-valign-middle-xl u-sheet-1">
                    <a href="/" class="u-align-center-md u-align-center-sm u-align-center-xs u-image u-logo u-image-1" data-image-width="240" data-image-height="170">
                        <img src="~/images/logo_medium.jpg" class="u-logo-image u-logo-image-1">
                    </a>
                    <form method="get" action="/Home/Search" class="form-control u-expanded-width-md u-expanded-width-sm u-expanded-width-xs u-grey-10 u-radius-50 u-search u-search-left u-search-1">
                        <button class="u-search-button" type="submit">
                            <span class="u-search-icon u-spacing-13">
                                <i class="fa-solid fa-magnifying-glass"></i>
                            </span>
                        </button>
                        <input class="u-custom-font u-font-roboto-condensed u-search-input u-text-black" type="search" name="searchProduct" value="" placeholder="Tìm kiếm sản phẩm...">
                    </form>
                    <div class="u-clearfix u-group-elements u-hidden-md u-hidden-sm u-hidden-xs u-group-elements-1">
                        <h4 class="u-align-left u-custom-font u-font-roboto-condensed u-text u-text-default u-text-1">Hổ trợ khách hàng </h4><span class="u-file-icon u-icon u-icon-1"><img src="~/images/5585856.png" alt=""></span>
                        <p class="u-align-center u-custom-font u-font-roboto-condensed u-text u-text-default u-text-2">
                            <a class="u-active-none u-border-none u-btn u-button-link u-button-style u-hover-none u-none u-text-black u-text-hover-custom-color-1 u-btn-1" href="tel:1800 6268">1800 6268</a>
                        </p>
                    </div>
                    <div class="u-container-align-center-xs u-container-style u-group u-group-1">
                        <div class="u-container-layout">
                            @{
                                var user = (@WebBanHangMeatDeli.Models.tb_Users)HttpContext.Current.Session["auth"];
                                if (user != null)
                                {
                                    <p class="u-align-center-xs u-custom-font u-font-roboto-condensed u-text u-text-3" style="margin: 18px 18px 0 23px;">@user.Name</p>
                                    @*<span class="u-align-center-xs u-file-icon u-hidden-sm u-hidden-xs u-icon u-text-custom-color-1 u-icon-2"><img src="~/images/64572-71a3609c.png" alt=""></span>*@
                                    <p class="u-align-center-xs u-custom-font u-font-roboto-condensed u-text u-text-4" style="margin: 0px 0px 0 24px;">
                                        <a class="u-btn u-button-link u-button-style u-none u-text-black u-text-hover-custom-color-1 u-btn-3" href="/account/logout">Đăng Xuất</a>
                                    </p>
                                }
                                else
                                {
                                    <p class="u-align-center-xs u-custom-font u-font-roboto-condensed u-text u-text-3">
                                        <a class="u-active-none u-border-none u-btn u-button-link u-button-style u-hover-none u-none u-text-black u-text-hover-custom-color-1 u-btn-2" href="/account/login">Đăng nhập</a>
                                    </p><span class="u-align-center-xs u-file-icon u-hidden-sm u-hidden-xs u-icon u-text-custom-color-1 u-icon-2"><img src="~/images/64572-71a3609c.png" alt=""></span>
                                    <p class="u-align-center-xs u-custom-font u-font-roboto-condensed u-text u-text-4">
                                        <a class="u-btn u-button-link u-button-style u-none u-text-black u-text-hover-custom-color-1 u-btn-3" href="/account/register">Đăng ký</a>
                                    </p>
                                }
                            }
                        </div>
                    </div><!--shopping_cart-->
                    <a class="u-align-center-xs u-shopping-cart u-shopping-cart-1" href="/cart">
                        <span class="u-custom-color-1 u-icon u-icon-circle u-shopping-cart-icon u-text-white u-icon-3">
                            <i class="fa-solid fa-cart-shopping" style="padding: 5px 0px 0px 1px"></i>
                            <span id="shoppingCount" class="u-custom-color-1 u-custom-font u-font-roboto-condensed u-icon-circle u-shopping-cart-count" style="font-size: 0.875rem;">0</span>
                        </span>
                    </a><!--/shopping_cart-->
                </div>
                <span style="height: 64px; width: 64px; margin-left: 0px; margin-right: auto; margin-top: 0px; background-image: none; right: 20px; bottom: 20px; padding: 15px; display: block;" class="u-back-to-top u-border-2 u-border-custom-color-1 u-custom-color-1 u-icon u-icon-circle u-opacity u-opacity-85" data-href="#">
                    <svg class="u-svg-link" preserveAspectRatio="xMidYMin slice" viewBox="0 0 551.13 551.13"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#svg-1d98"></use></svg>
                    <svg class="u-svg-content" enable-background="new 0 0 551.13 551.13" viewBox="0 0 551.13 551.13" xmlns="http://www.w3.org/2000/svg" id="svg-1d98"><path d="m275.565 189.451 223.897 223.897h51.668l-275.565-275.565-275.565 275.565h51.668z"></path></svg>
                </span>
                <style class="u-sticky-style" data-style-id="dc01">
                    .u-sticky-fixed.u-sticky-dc01:before, .u-body.u-sticky-fixed .u-sticky-dc01:before {
                        borders: top right bottom left !important;
                        border-color: #404040 !important;
                        border-width: 2px !important
                    }
                </style>
            </div>
            <div class="u-custom-color-1 u-section-row u-sticky u-sticky-1fa9 u-section-row-2" id="sec-bca1">
                <div class="u-clearfix u-sheet u-sheet-2">
                    <nav class="u-align-left u-dropdown-icon u-menu u-menu-dropdown u-offcanvas u-menu-1" data-submenu-level="on-click">
                        <div class="menu-collapse u-custom-font u-font-roboto-condensed" style="font-size: 0.9375rem; text-transform: uppercase;">
                            <a class="u-button-style u-custom-color u-custom-hover-color u-custom-text-active-color u-custom-text-color u-nav-link" href="#">
                                <i class="fa-sharp fa-solid fa-bars"></i>
                            </a>
                        </div>
                        <div class="u-custom-menu u-nav-container">
                            @Html.Action("CategoryHeader", "Category")

                        </div>
                        <div class="u-custom-menu u-nav-container-collapse">
                            <div class="u-black u-container-style u-inner-container-layout u-opacity u-opacity-95 u-sidenav">
                                <div class="u-inner-container-layout u-sidenav-overflow">
                                    <div class="u-menu-close"></div>
                                    <ul class="u-align-center-lg u-align-center-xl u-align-left-md u-align-left-sm u-align-left-xs u-custom-font u-font-roboto-condensed u-nav u-popupmenu-items u-spacing-5 u-text-hover-custom-color-1 u-unstyled u-nav-3">
                                        @Html.Action("CategoryHeader", "Category")
                                    </ul>
                                </div>
                            </div>
                            <div class="u-black u-menu-overlay u-opacity u-opacity-70"></div>
                        </div>
                    </nav>
                </div>
            </div>
        </div>
    </header>
    @RenderBody()
    @*Footer*@
